package CollectionFramework;

import java.util.ArrayList;
import java.util.ListIterator;

public class HasPrevious {

	public static void main(String[] args) {
		ArrayList<X> arrayList=new ArrayList<X>();
		
		for(int i=0;i<5;i++) {
			
			X a=new X();
			a.i=i;
			arrayList.add(a);
			
			System.out.println(i);
			
		}
	
	   }
		
	
}
