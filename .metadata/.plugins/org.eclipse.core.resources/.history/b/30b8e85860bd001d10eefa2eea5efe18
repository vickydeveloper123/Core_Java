package CollectionFramework;

import java.util.ArrayList;
import java.util.ListIterator;

public class HasPrevious {

	public static void main(String[] args) {
		ArrayList<String> arrayList=new ArrayList<String>();
		
		for(int j=0;j<=5;j++) {
			
			X a=new X();
			a.i=j;
			
			System.out.println("NumbersList ="+j);
			
			System.out.println("+====\\");
			System.out.println("ForwordTraversing");
			
			ListIterator<String> listIterater=arrayList.listIterator();
			while(listIterater.hasNext()) {
		     	String a1=listIterater.next();
		     	System.out.println(a1);
			}
			
		}
		
	}
}
